Cell[BoxData[
 RowBox[{
  RowBox[{"DtaExptList", "[", "dtaDirin_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"dtaDir", "=", "dtaDirin"}], ",", "dtafile", ",", "dtadata", 
      ",", "exptlist"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"dtafile", "=", 
      RowBox[{

Cell["\<\
read experimental information in the PDFset through .dta files \
\>", "Subsubsection",
 CellChangeTimes->{{3.703094650280985*^9, 3.703094728217774*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"DtaExptList", "[", "dtaDirin_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"dtaDir", "=", "dtaDirin"}], ",", "dtafile", ",", "dtadata", 
      ",", "exptlist", ",", "exptlistcheck", ",", "errorbool"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"dtafile", "=", 
      RowBox[{"FileNames", "[", 
       RowBox[{"dtaDir", "<>", "\"\<*dta\>\""}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "dtafile", "]"}], "\[Equal]", "0"}], ",", 
       RowBox[{
        RowBox[{"Print", "[", 
         RowBox[{
         "\"\<error, there is no .dta file under the directory \>\"", ",", 
          "dtaDir"}], "]"}], ";", 
        RowBox[{"Abort", "[", "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"assume", " ", "the", " ", "data", " ", 
       RowBox[{"in", " ", ".", "dta"}], " ", "file", " ", "is", " ", 
       "\"\<ct2016 format\>\""}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"dtadata", "=", 
      RowBox[{"ReadExptTable", "[", 
       RowBox[{
        RowBox[{"dtafile", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", "\"\<ct2016\>\""}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"exptlist", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"dtadata", "[", 
         RowBox[{"[", 
          RowBox[{"iexpt", ",", "1"}], "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"iexpt", ",", "1", ",", 
          RowBox[{"Length", "[", "dtadata", "]"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"check", " ", "all", " ", "sets", " ", 
       RowBox[{"of", " ", ".", "dta"}], " ", "files", " ", "have", " ", "the",
        " ", "same", " ", "expt", " ", "ID"}], "*)"}], "\[IndentingNewLine]", 
     
     RowBox[{"exptlistcheck", "=", "\[IndentingNewLine]", 
      RowBox[{"Table", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"dtadata", "=", 
          RowBox[{"ReadExptTable", "[", 
           RowBox[{
            RowBox[{"dtafile", "[", 
             RowBox[{"[", "ifile", "]"}], "]"}], ",", "\"\<ct2016\>\""}], 
           "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"dtadata", "[", 
            RowBox[{"[", 
             RowBox[{"iexpt", ",", "1"}], "]"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"iexpt", ",", "1", ",", 
             RowBox[{"Length", "[", "dtadata", "]"}]}], "}"}]}], "]"}]}], ",",
         "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"ifile", ",", 
          RowBox[{"dtafile", "//", "Length"}]}], "}"}]}], 
       "\[IndentingNewLine]", "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"errorbool", "=", "False"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Table", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"exptlistcheck", "[", 
            RowBox[{"[", "ifile", "]"}], "]"}], "!=", "exptlist"}], ",", 
          RowBox[{
           RowBox[{
           "Print", "[", 
            "\"\<error, Expt IDs in .dta files are different\>\"", "]"}], ";", 
           RowBox[{"errorbool", "=", "True"}]}]}], " ", "]"}], ";", 
        "\"\<dummy\>\""}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"ifile", ",", 
         RowBox[{"dtafile", "//", "Length"}]}], "}"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
